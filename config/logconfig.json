{
    "version": 1,
    "disable_existing_loggers": false,
    "handlers": {
        "default": {
            "class": "rich.logging.RichHandler",
            "show_time": true,
            "show_path": true,
            "rich_tracebacks": true
        }
    },
    "loggers": {
        "_granian": {
            "level": "INFO",
            "handlers": [
                "default"
            ],
            "propagate": false
        },
        "granian.access": {
            "level": "INFO",
            "handlers": [
                "default"
            ],
            "propagate": false
        },
        "httpx": {
            "level": "INFO",
            "handlers": [
                "default"
            ]
        },
        "httpcore": {
            "level": "INFO",
            "handlers": [
                "default"
            ]
        },
        "app": {
            "level": "DEBUG",
            "handlers": [
                "default"
            ],
            "propagate": false
        }
    }
}
